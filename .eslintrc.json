{
    "extends": "airbnb",
    "installedESLint": true,
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "skipBlankLines": true,
    "rules": {
      "arrow-body-style": ["error", "as-needed", { "requireReturnForObjectLiteral": true }],
      "react/forbid-prop-types": ["error", { "forbid": ["any"] }],
      "class-methods-use-this": "off"
    }
}
